	<!-- 典型案例 -->
	<div class="typical_cases" ng-if="zx.DXAL.status==0">
		<p>
			<span>{{zx.DXAL.showName}}</span>
			<a target="_blank" href="{{zx.DXAL | moreUrl}}" class="fr" ng-if="!!zx.DXAL.jumpUrl">更多</a>
		</p>
		<div class="typical_box">
			<div class="slideTxtBox anli_slideTxtBox">
			    <div class="bd">
			        <ul>
